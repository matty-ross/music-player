<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Artist</th>
            <th>File</th>
        </tr>
    </thead>
    <tbody>
        <% for (const song of songs) { %>
            <tr>
                <td><%= song.name %></td>
                <td><%= song.artist %></td>
                <td><%= song.file_name %></td>
            </tr>
        <% } %>
    </tbody>
</table>